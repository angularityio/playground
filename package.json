{
  "name": "playground",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:1": "ng build --base-href . --app hello-world",
    "build:2": "ng build --base-href . --app hello-world2",
    "build:3": "ng build --base-href . --app example-001-httpclient-testing",
    "build:ci": "npm run build:1 && npm run build:2 && npm run build:3 && cp index.html dist",
    "test1": "ng test --app hello-world --watch false --code-coverage",
    "test2": "ng test --app hello-world2 --watch false --code-coverage",
    "test3": "ng test --app example-001-httpclient-testing --watch false --code-coverage",
    "test:all": "npm run test1 && npm run test2 && npm run test3",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --single-quote --print-width 120 --write \"{apps,libs}/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/cdk": "^5.0.1",
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/material": "^5.0.1",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/router": "5.0.0",
    "@ngrx/effects": "^4.1.0",
    "@ngrx/router-store": "^4.1.0",
    "@ngrx/store": "^4.1.0",
    "@ngrx/store-devtools": "4.0.0",
    "@nrwl/nx": "0.2.2",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "nrwl/fix-cli-build",
    "@angular/compiler-cli": "5.0.0",
    "@angular/language-service": "5.0.0",
    "@nrwl/schematics": "0.2.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.7.4",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "2.5.3"
  }
}
